{
  "kind": "collectionType",
  "collectionName": "issues",
  "info": {
    "singularName": "issue",
    "pluralName": "issues",
    "displayName": "Issue",
    "name": "issue"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "issue"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
